    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-vkbansal": "^0.1.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4"
