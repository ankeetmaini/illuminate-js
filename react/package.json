{
  "name": "react-illuminate",
  "version": "1.1.0",
  "description": "Syntax Highlighting for React",
  "main": "lib/index.js",
  "scripts": {
    "compile:es5": "rm -rf ./lib && ../node_modules/.bin/babel --presets=react,es2015 --plugins=transform-class-properties ./src -d ./lib",
    "compile:es6": "rm -rf ./es6 && ../node_modules/.bin/babel --presets=react,es2016 --plugins=transform-class-properties ./src -d ./es6",
    "compile": "npm run compile:es5 && npm run compile:es6",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkbansal/illuminate-js.git"
  },
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vkbansal/illuminate-js/issues"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "illuminate-js": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "react": "^15.4.1"
  }
}
